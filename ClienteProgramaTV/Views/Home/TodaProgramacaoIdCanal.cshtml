@model IEnumerable<ClienteProgramaTV.programacaotv.Programa>

@{
    ViewBag.Title = "TodaProgramacaoIdCanal";
}

<h2>TodaProgramacaoIdCanal</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descricao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataHora)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descricao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataHora)
        </td>
        <td>                        
            @Html.ActionLink("Delete", "DeletePrograma", new { id=item.Id })
        </td>
    </tr>
}

</table>
